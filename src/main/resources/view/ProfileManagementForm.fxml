<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.dep9.clinic.controller.ProfileManagementFormController">
   <children>
      <Label layoutX="32.0" layoutY="39.0" text="PROFILE MANAGEMENT">
         <font>
            <Font name="Cantarell Extra Bold" size="20.0" />
         </font>
      </Label>
      <TableView fx:id="tblPeopleManagement" layoutX="32.0" layoutY="85.0" prefHeight="200.0" prefWidth="436.0">
        <columns>
          <TableColumn prefWidth="75.0" text="No" />
          <TableColumn prefWidth="75.0" text="Type" />
            <TableColumn prefWidth="75.0" text="Name" />
            <TableColumn prefWidth="101.0" text="Address" />
            <TableColumn prefWidth="105.0" text="Contact Details" />
        </columns>
      </TableView>
      <Button fx:id="btnAdd" defaultButton="true" layoutX="481.0" layoutY="85.0" mnemonicParsing="false" onAction="#btnAddOnAction" prefHeight="25.0" prefWidth="124.0" text="ADD">
         <font>
            <Font name="Cantarell Extra Bold" size="13.0" />
         </font>
      </Button>
      <Button fx:id="btnModify" defaultButton="true" layoutX="481.0" layoutY="125.0" mnemonicParsing="false" onAction="#btnModifyOnAction" prefHeight="25.0" prefWidth="124.0" text="MODIFY">
         <font>
            <Font name="Cantarell Extra Bold" size="13.0" />
         </font>
      </Button>
      <Button fx:id="btnDelete" defaultButton="true" layoutX="481.0" layoutY="166.0" mnemonicParsing="false" onAction="#btnDeleteOnAction" prefHeight="25.0" prefWidth="124.0" text="DELETE">
         <font>
            <Font name="Cantarell Extra Bold" size="13.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
