<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.dep9.clinic.controller.LoginFormController">
   <children>
      <AnchorPane layoutX="10.0" layoutY="10.0" prefHeight="400.0" prefWidth="600.0">
         <children>
            <Label layoutX="137.0" layoutY="53.0" text="MEDICARE CHANELING CENTER">
               <font>
                  <Font name="Cantarell Extra Bold" size="20.0" />
               </font>
            </Label>
            <VBox alignment="TOP_CENTER" layoutX="131.0" layoutY="100.0" prefHeight="277.0" prefWidth="356.0" spacing="20.0">
               <children>
                  <Label text="Username">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Label>
                  <TextField fx:id="txtUserName" />
                  <Label layoutX="156.0" layoutY="140.0" lineSpacing="10.0" text="Password">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Label>
                  <PasswordField fx:id="txtPassword" />
                  <Button fx:id="btnLogIn" defaultButton="true" lineSpacing="20.0" mnemonicParsing="false" onAction="#btnLogInOnAction" text="LOG IN" />
               </children>
            </VBox>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
